{
    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "target": "es5",
        "module": "esnext",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "*": ["src/*"]
        }
    },
    "include": ["src"]
}
